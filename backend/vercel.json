{
  "version": 2,
  "builds": [
    {
      "src": "kebede_pos/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "kebede_pos/wsgi.py"
    },
    {
      "src": "/admin/(.*)",
      "dest": "kebede_pos/wsgi.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "kebede_pos/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "kebede_pos.vercel_settings"
  }
}



